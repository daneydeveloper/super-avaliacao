    var t, i, n, r;
    com.site.Util.trackPage("home");
    $(".headerHomeLink, .headerHowItWorksLink, .headerTestimonialsLink").attr("href", "#");
    attachScrolling();
    subscribe();
    t = !$("#hdnSocialNetworkDataResponse").val() ? "" : unescape($("#hdnSocialNetworkDataResponse").val());
    i = !$("#hdnRecentlyPeddleCars").val() ? "" : unescape($("#hdnRecentlyPeddleCars").val());
    !t || getSocialNetworkDataCallback($.parseJSON(t));
    !i || getRecentlyPeddleCarInfoCallback($.parseJSON(i));
    n = window.location.hash;
    n + "" == "#how-it-works" ? $(".headerHowItWorksLink").click() : n + "" == "#testimonials" ? $(".headerTestimonialsLink").click() : com.site.Util.checkDigits(n.replace(/^\D+/g, "")) && (window.location = "/");
    $("#peddleVideo_html5_api").css("height", "0");
    $("#peddleVideo_html5_api").css("position", "absolute");
    $(".peddleHeader").addClass("mm-fixed-top");
    $(".getAnOffer").click(function(n) {
        return window.open(com.site.Config.VehicleInfoLink, "_self"), n.stopPropagation(), !1
    });
    r = navigator.userAgent.match(/iPad/i) != null;
    $("#watchVideo, .getAnOffer").bind("touchstart touchend", function() {
        $(this).toggleClass("buttonTapClass")
    });
    $(".buyaNewCarLink").bind("touchstart touchend", function() {
        $(this).toggleClass("buyaNewCarLinkHover")
    });
    $("#watchVideo").click(function() {
        $("#peddleVideo").addClass("peddleVideo");
        com.site.Home.isScriptAlreadyIncluded("/Scripts/Plugins/VideoPlugin/video.js") || function(n) {
            var t, i = "videoPluginId";
            n.getElementById(i) || (t = n.createElement("script"), t.id = i, t.async = !0, t.src = "/Scripts/Plugins/VideoPlugin/video.js", n.getElementsByTagName("head")[0].appendChild(t))
        }(document);
        var n = function() {
            "undefined" == typeof $(".video-js")[0].player || "undefined" == typeof jQuery ? window.setTimeout(n, 50) : com.site.Home.videoPlay()
        };
        n()
    });
    $("#videoPopupClose").click(function() {
        $("#peddleVideoModal").animate({
            height: 0
        }, {
            duration: 500,
            start: function() {
                $(".video-js")[0].player.pause();
                $("#peddleVideoModal").css("min-height", "0")
            },
            step: function() {},
            complete: function() {
                $(".video-js").css("opacity", "0");
                $(".peddleHeader").addClass("mm-fixed-top");
                $(".peddleVideoModal").css("z-index", "1");
                $("#peddleVideoModal .container").css("padding", "0");
                $("#peddleVideo_html5_api").css("height", "0");
                $("#peddleVideo_html5_api").css("position", "absolute");
                $("#peddleVideo").removeClass("peddleVideo")
            }
        })
    });
    com.site.Util.shuffleSelection(".shuffPopularVehicles");
    com.site.Util.shuffleSelection(".shuffPopularSearches");
    com.site.Util.shuffleSelection(".shuffPopularCity");
    com.site.Menu.checkForPubliserIdAvailability();
    $(".cloudContainer, .city, .roadContainer, #howItWorks, .mapStyleBg, #testimonials, .greyBgStyle, .footerContainer").bind("touchstart", function() {
        $(".headerPopover").tooltipster("hide")
    });
    $(".illustration, div.mapStyleBg, div.chatterImgBG").lazyload({
        threshold: 200
    })
})