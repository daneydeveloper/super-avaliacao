com.site.Home.videoPlay = function() {
    $("#peddleVideo_html5_api").css("height", "auto");
    $("#peddleVideo_html5_api").css("position", "relative");
    $("#peddleVideoModal").animate({
        height: $("#peddleVideo").height() + 80
    }, {
        duration: 500,
        start: function() {
            $(window).scrollTop(0);
            $(".video-js").css("opacity", "1");
            $(".video-js")[0].player.play();
            $(".peddleVideoModal").css("z-index", "1000");
            $("#peddleVideoModal .container").css("padding", "125px 0")
        },
        step: function() {},
        complete: function() {
            $("#peddleVideoModal").css("height", "auto");
            var n = $(".road").offset().top + $(".road").height() - $(".peddleHeader").height();
            $("#peddleVideoModal").css("min-height", n);
            $(".peddleHeader").removeClass("mm-fixed-top")
        }
    });
    attatchVideoController()
};
