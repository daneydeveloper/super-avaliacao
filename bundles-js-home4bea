function ipadFullScreenFix() {
    $(".vjs-fullscreen-control").after('<div class="fullScreenIcon icon-resize-full" tabindex="0"><\/div>');
    $(".vjs-fullscreen-control").hide();
    $(".customVolumeIcon ").hide();
    ipadFullScreen = !1;
    $(".fullScreenIcon").bind("click tap", function() {
        $("#peddleVideo_html5_api").toggleClass("ipadFix");
        $("#peddleVideo").toggleClass("ipadFixParent");
        $(".peddleVideoModal").toggleClass("peddleVideoModalFullScreen");
        var n = parseInt($("#peddleVideo_html5_api").css("height")) > 150 ? "150px" : $(window).height() - 250;
        $("#peddleVideo_html5_api").css("height", n);
        ipadFullScreen = !ipadFullScreen;
        $("body").toggleClass("bodyScroll");
        $("html").toggleClass("bodyScroll");
        $("#videoPopupClose").toggle();
        $(".cloudContainer").toggle();
        $(".peddleHeader").toggle()
    })
}

function attatchVideoController() {
    var n = navigator.userAgent.match(/iPad/i) != null;
    $("#videoVolumeSliderId").length > 0 || ($(".vjs-mute-control").after('<div id="videoVolumeSliderId" class="customVolumeIcon icon-volume" tabindex="0"><\/div>'), $(".vjs-mute-control").hide(), $(".customVolumeIcon").tooltipster({
        content: $("#volumeControl").html(),
        contentAsHTML: !0,
        interactive: !0,
        theme: "volumeControl-theme",
        functionReady: function() {
            $(".filterSlider").slider();
            lastAdjustedVolume !== "" && $(".filterSlider").slider("setValue", lastAdjustedVolume);
            $(".filterSlider").on("slide slideStop", function(n) {
                var t = this;
                return setTimeout(function() {
                    var n = $(t).val();
                    $(".video-js")[0].player.volume(1 - n / 100);
                    lastAdjustedVolume = n
                }, 400, t), n.stopPropagation(), !1
            });
            $(".volumeControl-theme").bind("mouseover", function() {
                $(".vjs-control-bar").css("cssText", "display: block !important;");
                $(".vjs-control-bar").css("opacity", "1");
                $(".vjs-control-bar").css("visibility", "visible")
            });
            $(".volumeControl-theme").bind("mouseout", function() {
                $(".vjs-control-bar").css("cssText", "display: block;")
            })
        }
    }), n && ipadFullScreenFix())
}

function attachScrolling() {
    $(".scroll-link").on("click", function(n) {
        n.preventDefault();
        var t = $(this).attr("data-id");
        scrollToID("#" + t, 750)
    });
    $(".scroll-top").on("click", function(n) {
        n.preventDefault();
        $("html, body").animate({
            scrollTop: 0
        }, "slow")
    });
    $("#nav-toggle").on("click", function(n) {
        n.preventDefault();
        $("#menu").toggleClass("open")
    })
}

function scrollToOffset(n, t) {
    var i = $(".peddleHeader").height(),
        r = $(n).offset().top - i;
    $("html,body").animate({
        scrollTop: r
    }, t)
}

function scrollToID(n, t) {
    $("#menu").off("closed.mm");
    $("#menu").on("closed.mm", function() {
        scrollToOffset(n, t)
    });
    var i = $("#menu");
    i.hasClass("open") ? i.trigger("close") : scrollToOffset(n, t)
}

function swiftlyScrolling() {
    var n = $.superscrollorama({
        triggerAtCenter: !1,
        playoutAnimations: !0
    });
    n.addTween("#browserIphone", (new TimelineLite).append([TweenMax.from($("#browserIphone"), "1", {
        css: {
            left: -292
        },
        delay: "2",
        ease: Quad.easeOut,
        immediateRender: !0
    })]), 800, -($(window).height() + $("#browserIphone").height()));
    n.addTween("#carrier", (new TimelineLite).append([TweenMax.from($("#carrier"), "1", {
        css: {
            left: -292
        },
        delay: "2",
        ease: Quad.easeOut,
        immediateRender: !0
    })]), 800, -($(window).height() + $("#carrier").height()));
    n.addTween("#offerCar", (new TimelineLite).append([TweenMax.from($("#offerCar"), "1", {
        css: {
            right: -292
        },
        delay: "2",
        ease: Quad.easeOut,
        immediateRender: !0
    })]), 800, -($(window).height() + $("#offerCar").height()));
    n.addTween("#check", (new TimelineLite).append([TweenMax.from($("#check"), "1", {
        css: {
            right: -292
        },
        delay: "2",
        ease: Quad.easeOut,
        immediateRender: !0
    })]), 800, -($(window).height() + $("#check").height() + 300))
}

function getRecentlyPeddleCarInfoCallback(n) {
    n.Success && !!n.Vehicles && amplify.publish("recentlyPeddleCarInfo.response", n, "recentlyPeddleCarInfo")
}

function getSocialNetworkDataCallback(n) {
    n.Success && amplify.publish("socialNetworkData.response", n, "socialNetworkData")
}

function subscribe() {
    amplify.subscribe("socialNetworkData.response", function(n) {
        setDataInTestimonials(n)
    });
    amplify.subscribe("recentlyPeddleCarInfo.response", function(n) {
        setDataForRecentlyPeddleCarInfo(n)
    })
}

function highLightPeddleText(n) {
    var t = n.search("@peddle");
    return t >= 0 ? (n = n.replace("@peddle", "<a href='https://twitter.com/peddle' target='_blank'><span class='greyTextStyle'>@peddle<\/span><\/a>"), n.length > 235) ? n.substring(0, 320) + "..." : n : n.length > 135 ? n.substring(0, 220) + "..." : n
}

function setDataInTestimonials(n) {
    var u = "",
        s = "",
        i = [],
        r = [],
        f = [],
        e, o, t, h;
    for ($.each(n.TweeterData, function(n, t) {
            var r = "";
            r += '<div class="tweetSlide">';
            r += '<div class="tweetStyle">';
            r += highLightPeddleText(t.Text);
            r += "<\/div>";
            r += '<div class="tweets">';
            r += '<a class="tweetsName" href="' + t.Link + '" target="_blank">' + t.Name + "<\/a>";
            r += " -- ";
            r += '<div class="tweeterSymbol">';
            r += "<\/div>";
            r += "<\/div>";
            r += "<\/div>";
            i.push(r)
        }), $.each(n.FacebookData, function(n, t) {
            var i = "";
            i += '<div class="tweetSlide">';
            i += '<div class="tweetStyle">';
            i += highLightPeddleText(t.Text);
            i += "<\/div>";
            i += '<div class="tweets">';
            i += '<a class="tweetsName"  href="' + t.Link + '" target="_blank">' + t.Name + "<\/a>";
            i += " -- ";
            i += '<div class="facebookSymbol"><i class="icon-facebook-circled-1"><\/i>';
            i += "<\/div>";
            i += "<\/div>";
            i += "<\/div>";
            r.push(i)
        }), shuffle(i), shuffle(r), e = 0, o = 0, t = 0; t < i.length + r.length; t++) e < r.length && (f.push(r[e]), e++), o < i.length && (f.push(i[o]), o++);
    for (u += '<div class="swipe-wrap">', t = 0; t < f.length && t < 4; t++) u += f[t], s += '<div class="socialPanelIndicator"><div><\/div><\/div>';
    u += "<\/div>";
    $("#tweet").append(u);
    $(".socialPanelIndicatorContainer").append(s);
    h = $("#tweet").Swipe({
        auto: 6e3,
        disableScroll: !1,
        speed: 200,
        stopPropagation: !1,
        indicator: !1,
        continuous: !0,
        startSlide: 0,
        callback: function(n) {
            $("#socialNetworkContainer .socialPanelIndicator .socialPanelIndicatorActive").removeClass("socialPanelIndicatorActive");
            $("#socialNetworkContainer .socialPanelIndicator").removeClass("socialPanelIndicatorActiveParent");
            $("#socialNetworkContainer .socialPanelIndicator").eq(n).children().addClass("socialPanelIndicatorActive");
            $("#socialNetworkContainer .socialPanelIndicator").eq(n).addClass("socialPanelIndicatorActiveParent")
        }
    }).data("Swipe");
    $("#socialNetworkContainer .socialPanelIndicator").eq(0).children().addClass("socialPanelIndicatorActive");
    $("#socialNetworkContainer .socialPanelIndicator").eq(0).addClass("socialPanelIndicatorActiveParent");
    $(".socialPanelIndicator").click(function() {
        if ($(this).children().hasClass("socialPanelIndicatorActive")) return !1;
        $(".socialPanelIndicatorActive").removeClass("socialPanelIndicatorActive");
        $(".socialPanelIndicatorActiveParent").removeClass("socialPanelIndicatorActiveParent");
        $(this).children().addClass("socialPanelIndicatorActive");
        $(this).addClass("socialPanelIndicatorActiveParent");
        h.slide($(this).index(), 200)
    });
    $(".swipe-wrap > div").css("float", "none");
    setTimeout(function() {
        $(".swipe-wrap > div").css("float", "left")
    }, 200)
}

function shuffle(n) {
    for (var t = n.length, r, i; 0 !== t;) i = Math.floor(Math.random() * t), t -= 1, r = n[t], n[t] = n[i], n[i] = r;
    return n
}

function setDataForRecentlyPeddleCarInfo(n) {
    var t = "",
        i;
    $.each(n.Vehicles, function(n, i) {
        t += '<div class="col-sm-4 col-md-4">';
        t += '<div class="carImageContainer img-responsive">';
        t += '<img id="carImage' + i.ClaimCode + '" src="' + i.CarImage + '" class="img-responsive carImageStyle" onError="com.site.Home.setDefaultImage(this.id)" />';
        t += '<div class="recentCarImage" style="background:url(' + i.CarImage + ')"><\/div>';
        t += '<div class="row">';
        t += '<div class="col-xs-6 col-sm-6 col-md-8">';
        t += '<div class="carNameStyle">';
        t += i.Year + " " + i.Make + " " + i.Model;
        t += "<\/div>";
        t += '<div class="carLocationStyle">' + i.City + ", " + i.StateCode;
        t += "<\/div>";
        t += "<\/div>";
        t += '<div class="col-xs-6 col-sm-6 col-md-4">';
        t += '<span class="badge ronniaFont">$' + com.site.Util.add_commas(i.OfferAmount);
        t += "<\/span>";
        t += "<\/div>";
        t += "<\/div>";
        t += "<\/div>";
        t += "<\/div>"
    });
    $("#recentlyPeddledContainerDesktop").append(t);
    t = "";
    t += '<div class="swipe-wrap">';
    $.each(n.Vehicles, function(n, i) {
        t += '<div class="recentlyPeddledContainer">';
        t += '<div class="carImageContainer img-responsive">';
        t += '<img id="carImage' + i.ClaimCode + '" src="' + i.CarImage + '" class="img-responsive carImageStyle" onError="com.site.Util.setDefaultImage(this.id)" />';
        t += '<div class="pull-left">';
        t += '<div class="carNameStyle">';
        t += i.Year + " " + i.Make + " " + i.Model;
        t += "<\/div>";
        t += '<div class="carLocationStyle">' + i.City + ", " + i.StateCode;
        t += "<\/div>";
        t += "<\/div>";
        t += '<span class="badge ronniaFont">$' + com.site.Util.add_commas(i.OfferAmount);
        t += "<\/span>";
        t += "<\/div>";
        t += "<\/div>"
    });
    t += "<\/div>";
    $("#recentlyPeddled").append(t);
    i = $("#recentlyPeddled").Swipe({
        disableScroll: !1,
        speed: 200,
        stopPropagation: !1,
        indicator: !1,
        continuous: !0,
        startSlide: 0,
        callback: function(n) {
            $("#recentlyPeddledContainer .panelIndicator .panelIndicatorActive").removeClass("panelIndicatorActive");
            $("#recentlyPeddledContainer .panelIndicator").removeClass("panelIndicatorActiveParent");
            $("#recentlyPeddledContainer .panelIndicator").eq(n).children().addClass("panelIndicatorActive");
            $("#recentlyPeddledContainer .panelIndicator").eq(n).addClass("panelIndicatorActiveParent")
        }
    }).data("Swipe");
    $(".panelIndicator").click(function() {
        if ($(this).children().hasClass("panelIndicatorActive")) return !1;
        $(".panelIndicatorActive").removeClass("panelIndicatorActive");
        $(".panelIndicatorActiveParent").removeClass("panelIndicatorActiveParent");
        $(this).children().addClass("panelIndicatorActive");
        $(this).addClass("panelIndicatorActiveParent");
        i.slide($(this).index(), 200)
    });
    $(".carImageContainer").click(function(n) {
        return window.open(com.site.Config.VehicleInfoLink, "_self"), n.stopPropagation(), !1
    })
}

function isiPhone() {
    return navigator.platform.indexOf("iPhone") != -1 || navigator.platform.indexOf("iPod") != -1
}
var ZIPCodeTimeZoneGBL, differentSellerAccount, com, lastAdjustedVolume, ipadFullScreen;
if (com) {
    if (typeof com != "object") throw new Error("com already exists and is not an object");
} else com = {};
if (com.site) {
    if (typeof com.site != "object") throw new Error("com.site already exists and is not an object");
} else com.site = {};
if (com.site.CreateAccount) {
    if (typeof com.site.CreateAccount != "object") throw new Error("com.site.CreateAccount already exists and is not an object");
} else com.site.CreateAccount = {};
if (com.site.CreateAccount.createAccountQuestionProperty = [{
        name: "createAccountEmailInput",
        isVisible: !0,
        answer: "",
        errorText: "email address",
        setAnswer: function(n) {
            this.answer = n
        },
        getAnswer: function() {
            return this.answer
        }
    }], com.site.CreateAccount.CreateAccountControl = function(n) {
        this.postEnableButton = n.postEnableButton;
        this.postDisableButton = n.postDisableButton;
        this.postClickEnableButton = n.postClickEnableButton;
        this.postClickDisableButton = n.postClickDisableButton;
        this.CAQId = com.site.CreateAccount.createAccountQuestionProperty;
        this.AttachCreateAccountEvents();
        this.MIN_PASSWORD_LENGTH = 6;
        this.ErrorMessageText = "";
        this.PreviousErrorMessageTextFilled = !1
    }, com.site.CreateAccount.CreateAccountControl.prototype = {
        getInputIndexFromName: function(n) {
            for (var t = 0; t < this.CAQId.length; t++)
                if (this.CAQId[t].name == n) return t;
            return null
        },
        getInputValue: function(n) {
            var t = this.getInputIndexFromName(n);
            return this.CAQId[t].getAnswer()
        },
        createAccountButtonAction: function() {
            this.validateAllInput(!1, !0);
            this.IsAllAnsweredSet() === !0 ? this.postClickEnableButton() : (this.ErrorMessageText = "", this.PreviousErrorMessageTextFilled = !1, this.highLightError(), this.postClickDisableButton())
        },
        highLightError: function() {
            this.validateAllInput(!0, !0)
        },
        hideCustomErrorAlert: function() {
            $("#createAccountErrorMsg .errorMessageText").html("");
            $("#createAccountErrorMsg").hide()
        },
        validateAllInput: function(n, t) {
            this.validateInput("createAccountEmailInput", n, t)
        },
        IsAllAnsweredSet: function() {
            for (var t, n = 0; n < this.CAQId.length; n++)
                if (t = this.CAQId[n], t.isVisible && t.answer == "") return n;
            return !0
        },
        validateInput: function(n, t, i) {
            var r = this.getInputIndexFromName(n),
                u = $.trim($("#" + n).val());
            if (this.CAQId[r].isVisible) {
                if (com.site.Util.isBlank(n)) return this.CAQId[r].setAnswer(""), i || (com.site.Util.removeErrorStateToInput(n, !0), this.hideCustomErrorAlert()), t && (com.site.Util.addErrorStateToInput(n, !0), (this.ErrorMessageText == "" || this.PreviousErrorMessageTextFilled) && (this.ErrorMessageText = this.ErrorMessageText != "" ? this.ErrorMessageText.replace(".", ",") + " " + this.CAQId[r].errorText + "." : "Enter valid " + this.CAQId[r].errorText + ".", this.PreviousErrorMessageTextFilled = !0)), !1;
                switch (n) {
                    case "createAccountEmailInput":
                        if (!com.site.Util.isValidEmail(u)) return this.CAQId[r].setAnswer(""), (i || t) && (com.site.Util.addErrorStateToInput(n, !0), this.ErrorMessageText == "" ? this.ErrorMessageText = "Enter valid email address." : ""), !1
                }
                this.CAQId[r].setAnswer($.trim($("#" + n).val()));
                com.site.Util.addSuccessStateToInput(n, !0)
            }
            return !0
        },
        enableButton: function() {
            this.postEnableButton()
        },
        disableButton: function() {
            this.postDisableButton()
        },
        checkAllAnswerSet: function() {
            if (this.IsAllAnsweredSet() === !0) {
                this.enableButton();
                return
            }
            this.disableButton()
        },
        resetAllInput: function() {
            $("#createAccountEmailInput").val("");
            com.site.Util.resetInputState("createAccountEmailInput", !0);
            this.validateAllInput(!1, !1);
            this.checkAllAnswerSet()
        },
        scrollTopInCharome: function(n) {
            jQuery.browser.mobile && !com.site.Util.isiPhone() && $(".modal-scrollable").animate({
                scrollTop: $(n).position().top - 50
            })
        },
        AttachCreateAccountEvents: function() {
            var n = this;
            n.resetAllInput();
            $("#createAccountEmailInput").bind({
                blur: function() {
                    var t = this;
                    setTimeout(function() {
                        n.validateInput(t.id, !1, !0);
                        n.checkAllAnswerSet()
                    }, 150, t)
                },
                keyup: function() {
                    com.site.Util.removeErrorStateToInput(this.id, !0);
                    n.hideCustomErrorAlert();
                    n.validateInput(this.id, !1);
                    n.checkAllAnswerSet()
                },
                keydown: function(t) {
                    var i = com.site.Util.getKeyCode(t),
                        r = this;
                    return setTimeout(function() {
                        i == com.site.Config.ENTER_KEY_CODE && (n.validateAllInput(!0, !0), n.IsAllAnsweredSet() === !0 && n.postClickEnableButton())
                    }, 0, r, i), i == com.site.Config.ENTER_KEY_CODE ? (t.stopPropagation(), t.preventDefault(), !1) : void 0
                },
                paste: function() {
                    var t = this;
                    setTimeout(function() {
                        com.site.Util.removeErrorStateToInput(this.id, !0);
                        n.hideCustomErrorAlert();
                        n.validateInput(t.id, !1, !1);
                        n.checkAllAnswerSet()
                    }, 50, t)
                },
                click: function() {
                    n.scrollTopInCharome(this)
                }
            })
        }
    }, com) {
    if (typeof com != "object") throw new Error("com already exists and is not an object");
} else com = {};
if (com.site) {
    if (typeof com.site != "object") throw new Error("com.site already exists and is not an object");
} else com.site = {};
if (com.site.Login) {
    if (typeof com.site.Login != "object") throw new Error("com.site.Login already exists and is not an object");
} else com.site.Login = {};
if (com.site.Login.loginQuestionProperty = [{
        name: "loginEmailInput",
        isVisible: !0,
        answer: "",
        errorText: "email address",
        setAnswer: function(n) {
            this.answer = n
        },
        getAnswer: function() {
            return this.answer
        }
    }, {
        name: "loginPasswordInput",
        isVisible: !0,
        answer: "",
        errorText: "password",
        setAnswer: function(n) {
            this.answer = n
        },
        getAnswer: function() {
            return this.answer
        }
    }], com.site.Login.LoginControl = function(n) {
        this.postEnableButton = n.postEnableButton;
        this.postDisableButton = n.postDisableButton;
        this.postClickEnableButton = n.postClickEnableButton;
        this.postClickDisableButton = n.postClickDisableButton;
        this.LQId = com.site.Login.loginQuestionProperty;
        this.AttachLoginEvents();
        this.MIN_PASSWORD_LENGTH = 6;
        this.ErrorMessageText = "";
        this.PreviousErrorMessageTextFilled = !1
    }, com.site.Login.LoginControl.prototype = {
        getInputIndexFromName: function(n) {
            for (var t = 0; t < this.LQId.length; t++)
                if (this.LQId[t].name == n) return t;
            return null
        },
        getInputValue: function(n) {
            var t = this.getInputIndexFromName(n);
            return this.LQId[t].getAnswer()
        },
        loginButtonAction: function() {
            this.validateInput("loginEmailInput", !1);
            this.validateInput("loginPasswordInput", !1);
            this.IsAllAnsweredSet() === !0 ? this.postClickEnableButton() : (this.ErrorMessageText = "", this.PreviousErrorMessageTextFilled = !1, this.highLightError(), this.postClickDisableButton())
        },
        highLightError: function() {
            this.validateInput("loginEmailInput", !0, !0);
            this.validateInput("loginPasswordInput", !0, !0)
        },
        hideCustomErrorAlert: function() {
            $("#loginErrorMsg .errorMessageText").html("");
            $("#loginErrorMsg").hide()
        },
        IsAllAnsweredSet: function() {
            for (var t, n = 0; n < this.LQId.length; n++)
                if (t = this.LQId[n], t.isVisible && t.answer == "") return n;
            return !0
        },
        validateInput: function(n, t, i) {
            var r = this.getInputIndexFromName(n),
                u = $.trim($("#" + n).val());
            if (com.site.Util.isBlank(n)) return this.LQId[r].setAnswer(""), i || (com.site.Util.removeErrorStateToInput(n, !0), this.hideCustomErrorAlert()), t && (com.site.Util.addErrorStateToInput(n, !0), (this.ErrorMessageText == "" || this.PreviousErrorMessageTextFilled) && (this.ErrorMessageText = this.ErrorMessageText != "" ? this.ErrorMessageText.replace(".", ",") + " " + this.LQId[r].errorText + "." : "Enter valid " + this.LQId[r].errorText + ".", this.PreviousErrorMessageTextFilled = !0)), !1;
            switch (n) {
                case "loginEmailInput":
                    if (!com.site.Util.isValidEmail(u)) return this.LQId[r].setAnswer(""), (i || t) && (com.site.Util.addErrorStateToInput(n, !0), this.ErrorMessageText == "" ? this.ErrorMessageText = "Enter valid email address." : ""), !1;
                    break;
                case "loginPasswordInput":
                    if (!com.site.Util.checkForValidPassword(u)) return this.LQId[r].setAnswer(""), com.site.Util.addErrorStateToInput(n, !0), this.ErrorMessageText == "" ? this.ErrorMessageText = "Invalid password character." : "", !1;
                    if (u.length < this.MIN_PASSWORD_LENGTH) return this.LQId[r].setAnswer(""), (i || t) && (com.site.Util.addErrorStateToInput(n, !0), this.ErrorMessageText == "" ? this.ErrorMessageText = "Passwords must be six or more characters." : ""), !1
            }
            return this.LQId[r].setAnswer($.trim($("#" + n).val())), com.site.Util.addSuccessStateToInput(n, !0), !0
        },
        enableButton: function() {
            this.postEnableButton()
        },
        disableButton: function() {
            this.postDisableButton()
        },
        checkAllAnswerSet: function() {
            if (this.IsAllAnsweredSet() === !0) {
                this.enableButton();
                return
            }
            this.disableButton()
        },
        resetAllInput: function() {
            $("#loginEmailInput, #loginPasswordInput").val("");
            com.site.Util.resetInputState("loginEmailInput", !0);
            com.site.Util.resetInputState("loginPasswordInput", !0);
            this.validateInput("loginEmailInput", !1, !1);
            this.validateInput("loginPasswordInput", !1, !1);
            this.checkAllAnswerSet()
        },
        scrollTopInCharome: function(n) {
            jQuery.browser.mobile && !com.site.Util.isiPhone() && $(".modal-scrollable").animate({
                scrollTop: $(n).position().top - 50
            })
        },
        AttachLoginEvents: function() {
            var n = this;
            n.resetAllInput();
            $("#loginEmailInput, #loginPasswordInput").bind({
                blur: function() {
                    var t = this;
                    setTimeout(function() {
                        var i = t.id == "loginEmailInput" || t.id == "loginPasswordInput" ? !0 : !1;
                        n.validateInput(t.id, !1, i);
                        n.checkAllAnswerSet()
                    }, 150, t)
                },
                keyup: function() {
                    com.site.Util.removeErrorStateToInput(this.id, !0);
                    n.checkAllAnswerSet();
                    n.hideCustomErrorAlert()
                },
                keydown: function(t) {
                    var i = com.site.Util.getKeyCode(t),
                        r = this;
                    return setTimeout(function() {
                        i == com.site.Config.ENTER_KEY_CODE && (n.validateInput("loginEmailInput", !1, !1), n.validateInput("loginPasswordInput", !1, !1), $("#loginPasswordInput").blur(), n.IsAllAnsweredSet() === !0 && n.postClickEnableButton())
                    }, 0, r, i), i == com.site.Config.ENTER_KEY_CODE ? (t.stopPropagation(), t.preventDefault(), !1) : void 0
                },
                paste: function() {
                    var t = this;
                    setTimeout(function() {
                        com.site.Util.removeErrorStateToInput(this.id, !0);
                        n.hideCustomErrorAlert();
                        n.validateInput(t.id, !1, !1);
                        n.checkAllAnswerSet()
                    }, 50, t)
                },
                click: function() {
                    n.scrollTopInCharome(this)
                }
            })
        }
