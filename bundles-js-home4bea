    }, $(document).ready(function() {
        infoPopUpModalInit = {
            setSuccessButtonAction: com.site.ForgotPassword.setInfoPopUpSuccessButtonAction,
            setCancelButtonAction: com.site.ForgotPassword.setInfoPopUpCancelButtonAction
